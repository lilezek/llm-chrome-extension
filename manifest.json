{
  "manifest_version": 3,
  "name": "Smart browsing",
  "description": "Base Level Extension",
  "version": "1.0",
  "action": {
    "default_popup": "UI/main.html"
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "sandbox": {
    "pages": [
      "UI/sandbox.html"
    ]
  }
}